{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "The app accesses your photos to let you upload a photo."
        }
      ]
    ],
    "name": "BeFocused",
    "scheme": "BeFocused",
    "slug": "BeFocused",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.fuyiqiao.BeFocused"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "65288ca8-aa30-427f-b972-298db887d684"
      }
    }
  }
}
